<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    style="@style/ColorFondoAplicacion"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="es.studium.opcion_pacientes.DatosDelDiagnosticoActivity">

    <ImageView
        android:id="@+id/btnVolver_DatosDelDiagnostico"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/icono_flecha" />

    <TextView
        android:id="@+id/PA_XDIA_lbl_titulo_DatosDelDiagnostico"
        style="@style/TextosAzul_tam30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="@string/PA_XDIA_lbl_titulo_DatosDelDiagnosticos"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/PA_XDIAG_lbl_apellidosNombre_DatosDelDiagnostico"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/PA_XDIA_lbl_apellidosNombre_DatosDelDiagnosticos"
        app:layout_constraintTop_toBottomOf="@id/PA_XDIA_lbl_titulo_DatosDelDiagnostico"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/PA_XDIAG_lbl_fecha_DatosDelDiagnostico"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/PA_XDIA_lbl_fecha_ResumenDiagnosticos"
        app:layout_constraintTop_toBottomOf="@id/PA_XDIAG_lbl_apellidosNombre_DatosDelDiagnostico"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <View
        android:id="@+id/PA_XDIAG_marcoFotoDiagnostico_DatosDelDiagnosticos"
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/rectangulo_azul"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/PA_XDIAG_lbl_fecha_DatosDelDiagnostico"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/PA_XDIAG_fotoDiagnostico_DatosDelDiagnosticos"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="10dp"
        android:scaleType="centerCrop"
        android:src="@drawable/icono_noimagen"
        app:layout_constraintBottom_toBottomOf="@+id/PA_XDIAG_marcoFotoDiagnostico_DatosDelDiagnosticos"
        app:layout_constraintEnd_toEndOf="@+id/PA_XDIAG_marcoFotoDiagnostico_DatosDelDiagnosticos"
        app:layout_constraintStart_toStartOf="@+id/PA_XDIAG_marcoFotoDiagnostico_DatosDelDiagnosticos"
        app:layout_constraintTop_toTopOf="@+id/PA_XDIAG_marcoFotoDiagnostico_DatosDelDiagnosticos"
        app:layout_constraintVertical_bias="0.699" />

    <TextView
        android:id="@+id/PA_XDIAG_lbl_diagnostico_DatosDelDiagnostico"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/PA_XDIA_lbl_diagnostico_DatosDelDiagnosticos"
        android:gravity="center"
        android:lineSpacingExtra="8dp"
        app:layout_constraintTop_toBottomOf="@id/PA_XDIAG_marcoFotoDiagnostico_DatosDelDiagnosticos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/PA_XDIAG_lbl_tipo_DatosDelDiagnostico"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/PA_XDIA_lbl_tipo_DatosDelDiagnosticos"
        android:gravity="center"
        android:lineSpacingExtra="8dp"
        app:layout_constraintTop_toBottomOf="@id/PA_XDIAG_lbl_diagnostico_DatosDelDiagnostico"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/PA_XDIAG_lbl_medico_DatosDelDiagnostico"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/PA_XDIA_lbl_medico_DatosDelDiagnosticos"
        android:gravity="center"
        android:lineSpacingExtra="8dp"
        app:layout_constraintTop_toBottomOf="@id/PA_XDIAG_lbl_tipo_DatosDelDiagnostico"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/PA_XDIAG_lbl_centro_DatosDelDiagnostico"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/PA_XDIA_lbl_nombreCentro_ResumenDiagnosticos"
        android:gravity="center"
        android:lineSpacingExtra="8dp"
        app:layout_constraintTop_toBottomOf="@id/PA_XDIAG_lbl_medico_DatosDelDiagnostico"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>