<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    style="@style/ColorFondoAplicacion"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="es.studium.opcion_informes.BuscadorInformesActivity">

    <ImageView
        android:id="@+id/INF_btnVolver_BuscadorInformes"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/icono_flecha" />

    <TextView
        android:id="@+id/INF_lbl_titulo_BuscadorInformes"
        style="@style/TextosAzul_tam30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="@string/INF_lbl_Titulo_BuscadorInformes"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_nombrePaciente_BuscadorInformes"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/INF_lbl_apellidosNombre_BuscadorInformes"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_titulo_BuscadorInformes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_nuhsaPaciente_BuscadorInformes"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/INF_lbl_nuhsa_BuscadorInformes"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_nombrePaciente_BuscadorInformes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_seleccionFecha_BuscadorInformes"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/INF_lbl_seleccionFecha_BuscadorInformes"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_nuhsaPaciente_BuscadorInformes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_fechaDesde_BuscadorInformes"
        style="@style/TextosAzul_tam16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/INF_lbl_FechaDesde_BuscadorInformes"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_seleccionFecha_BuscadorInformes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="8dp" />

    <EditText
        android:id="@+id/INF_txt_fechaDesde_BuscadorInformes"
        style="@style/EditTextRedondeado"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_fechaDesde_BuscadorInformes"
        android:hint="@string/INF_hint_FechaDesde_BuscadorInformes"
        android:inputType="date"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_fechaHasta_BuscadorInformes"
        style="@style/TextosAzul_tam16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/INF_txt_fechaDesde_BuscadorInformes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="8dp"
        android:text="@string/INF_lbl_FechaHasta_BuscadorInformes" />

    <EditText
        android:id="@+id/INF_txt_fechaHasta_BuscadorInformes"
        style="@style/EditTextRedondeado"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_fechaHasta_BuscadorInformes"
        android:hint="@string/INF_hint_FechaHasta_BuscadorInformes"
        android:inputType="date"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/INF_btn_BuscarDiagnosticos_BuscadorInformes"
        style="@style/EstiloBotonesLogin"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="16dp"
        android:text="@string/INF_btn_Buscar_BuscadorInformes"
        app:layout_constraintTop_toBottomOf="@+id/INF_txt_fechaHasta_BuscadorInformes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_SeleccionDiag_BuscadorInformes"
        style="@style/TextosAzul_tam30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/INF_btn_Buscar_BuscadorInformes"
        app:layout_constraintTop_toBottomOf="@id/INF_btn_BuscarDiagnosticos_BuscadorInformes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.appcompat.widget.AppCompatSpinner
        android:id="@+id/INF_spinner_seleccionDiag_BuscadorInformes"
        style="@style/EditTextRedondeado"
        android:popupBackground="@drawable/rectangulo_blanco"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/INF_lbl_SeleccionDiag_BuscadorInformes"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_DiagSeleccionado_BuscadorInformes"
        style="@style/TextosAzul_tam20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/INF_lbl_diagSeleccionado_BuscadorInformes"
        app:layout_constraintTop_toBottomOf="@id/INF_spinner_seleccionDiag_BuscadorInformes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_idDiag_BuscadorInformes"
        style="@style/TextosAzul_tam16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/INF_lbl_idDiag_BuscadorInformes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_DiagSeleccionado_BuscadorInformes" />

    <TextView
        android:id="@+id/INF_lbl_diadDiag_BuscadorInformes"
        style="@style/TextosAzul_tam16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/INF_lbl_diagDiag_BuscadorInformes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_idDiag_BuscadorInformes" />

    <TextView
        android:id="@+id/INF_lbl_tipolesion_BuscadorInformes"
        style="@style/TextosAzul_tam16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/INF_lbl_tipoLesion_BuscadorInformes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_diadDiag_BuscadorInformes" />

    <TextView
        android:id="@+id/INF_lbl_fechaDiag_BuscadorInformes"
        style="@style/TextosAzul_tam16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="@string/INF_lbl_fechaDiagnostico_BuscadorInformes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/INF_lbl_tipolesion_BuscadorInformes" />

    <Button
        android:id="@+id/INF_btn_visualizarInforme_BuscadorInformes"
        style="@style/EstiloBotonesLogin"
        android:layout_width="275dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:text="@string/INF_btn_visualizar_BuscadorInformes"
        app:layout_constraintBottom_toTopOf="@+id/INF_lbl_camposObligatorios_BuscadorInformes"
        android:layout_marginBottom="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/INF_lbl_camposObligatorios_BuscadorInformes"
        style="@style/TextosGris_tam16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/LO_lbl_camposObligatorios"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>